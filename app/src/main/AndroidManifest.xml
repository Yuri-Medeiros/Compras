<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Compras"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compras">
        <activity
            android:name=".view.NewItem"
            android:exported="false" />
        <activity
            android:name=".view.ItemList"
            android:exported="false" />
        <activity
            android:name=".view.ListDetail"
            android:exported="false" />
        <activity
            android:name=".view.NewList"
            android:exported="false" />
        <activity
            android:name=".view.ItemShop"
            android:exported="false" />
        <activity
            android:name=".view.HomeActivity"
            android:exported="false"
            android:theme="@style/Theme.Compras" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Compras">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.CadastroActivity"
            android:theme="@style/Theme.Compras" />
    </application>

</manifest>